{
  "name": "git-bomb",
  "version": "1.0.0",
  "description": "An Electron-based utility to run `git reset --HARD` on a timer",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "test": "mocha -w",
    "build-win": "electron-packager . --platform=win32 --arch=x64 --out=dist --icon=bomb --overwrite",
    "build-mac": "electron-packager . --platform=darwin --arch=x64 --out=dist --icon=bomb --overwrite"
  },
  "keywords": [
    "git"
  ],
  "author": "Allan Stewart",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "electron": "^1.4.15",
    "electron-packager": "^8.5.1",
    "mocha": "^3.2.0"
  }
}
